    "astro": "^4.0.3",
    "ky": "^1.1.3",
    "solid-js": "^1.4.3",
    "tailwindcss": "^3.0.24"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "prettier": "^3.1.1",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "solid-devtools": "^0.29.2",
    "wrangler": "^3.19.0"
  }
